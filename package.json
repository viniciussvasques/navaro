{
  "name": "navaro",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:api": "cd packages/api && python -m uvicorn app.main:app --reload",
    "dev:cliente": "cd apps/cliente && npx expo start",
    "dev:barbeiro": "cd apps/barbeiro && npx expo start",
    "dev:admin": "cd apps/admin && npm run dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo clean"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.0",
    "typescript": "^5.7.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
